{
  "name": "sanity-plugin-google-translate",
  "version": "3.0.0",
  "description": "Translate your content with Google Translate directly from your Sanity Studio",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/sanity-plugin-google-translate#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-google-translate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-google-translate.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "dependencies": {
    "@sanity/icons": "^2.2.2",
    "@sanity/incompatible-plugin": "^1.0.4",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@sanity/pkg-utils": "^2.2.13",
    "@sanity/plugin-kit": "^3.1.7",
    "@sanity/semantic-release-preset": "^4.1.0",
    "@types/react": "^18.0.31",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^4.4.1",
    "sanity": "^3.8.1",
    "styled-components": "^5.3.9",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
